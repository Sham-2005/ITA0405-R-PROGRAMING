lowercase_letters <- letters[1:10]
uppercase_letters <- toupper(letters[17:26])
letters_22_24_upper <- toupper(letters[22:24])
cat("First 10 lowercase letters: ", lowercase_letters, "\n")
cat("Last 10 uppercase letters: ", uppercase_letters, "\n")
cat("Letters between 22nd to 24th in uppercase: ", letters_22_24_upper, "\n")
